{
    "manifest_version": 2,
    "name": "Smag Jirlab extension",
    "version": "0.5.1",
    "permissions": ["activeTab", "tabs", "contextMenus", "storage", "notifications", "https://brodaleegitlabplugin.herokuapp.com/*", "https://gitlab.com/*"],
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": [
          "./scripts/Objects.js",
          "./scripts/functions.js",
          "./scripts/console.js",
          "./scripts/vars.js",
          "./lib/snackBar.js",
          "./lib/templateEngine.js",
          "./lib/vanillapicker.js",
          "script.js"
        ]
      }
    ],
    "icons": {
      "48": "/icons/iconx48.png",
      "96": "./icons/iconx96.png"
    },
    "background": {
      "scripts": [
        "./scripts/vars.js",
        "./scripts/Objects.js",
        "./scripts/backgroundScript.js"
      ]
    },
    "browser_action": {
      "default_popup": "popup.html"
    }
}